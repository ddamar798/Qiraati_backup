[
{
    "index": "078",
    "name": "an-Naba'",
    "verse": {
        "verse_0": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
        "verse_1": "عَمَّ يَتَسَآءَلُونَ",
        "verse_2": "عَنِ ٱلنَّبَإِ ٱلْعَظِيمِ",
        "verse_3": "ٱلَّذِى هُمْ فِيهِ مُخْتَلِفُونَ",
        "verse_4": "كَلَّا سَيَعْلَمُونَ",
        "verse_5": "ثُمَّ كَلَّا سَيَعْلَمُونَ",
        "verse_6": "أَلَمْ نَجْعَلِ ٱلْأَرْضَ مِهَٰدًۭا",
        "verse_7": "وَٱلْجِبَالَ أَوْتَادًۭا",
        "verse_8": "وَخَلَقْنَٰكُمْ أَزْوَٰجًۭا",
        "verse_9": "وَجَعَلْنَا نَوْمَكُمْ سُبَاتًۭا",
        "verse_10": "وَجَعَلْنَا ٱلَّيْلَ لِبَاسًۭا",
        "verse_11": "وَجَعَلْنَا ٱلنَّهَارَ مَعَاشًۭا",
        "verse_12": "وَبَنَيْنَا فَوْقَكُمْ سَبْعًۭا شِدَادًۭا",
        "verse_13": "وَجَعَلْنَا سِرَاجًۭا وَهَّاجًۭا",
        "verse_14": "وَأَنزَلْنَا مِنَ ٱلْمُعْصِرَٰتِ مَآءًۭ ثَجَّاجًۭا",
        "verse_15": "لِّنُخْرِجَ بِهِۦ حَبًّۭا وَنَبَاتًۭا",
        "verse_16": "وَجَنَّٰتٍ أَلْفَافًا",
        "verse_17": "إِنَّ يَوْمَ ٱلْفَصْلِ كَانَ مِيقَٰتًۭا",
        "verse_18": "يَوْمَ يُنفَخُ فِى ٱلصُّورِ فَتَأْتُونَ أَفْوَاجًۭا",
        "verse_19": "وَفُتِحَتِ ٱلسَّمَآءُ فَكَانَتْ أَبْوَٰبًۭا",
        "verse_20": "وَسُيِّرَتِ ٱلْجِبَالُ فَكَانَتْ سَرَابًا",
        "verse_21": "إِنَّ جَهَنَّمَ كَانَتْ مِرْصَادًۭا",
        "verse_22": "لِّلطَّٰغِينَ مَـَٔابًۭا",
        "verse_23": "لَّٰبِثِينَ فِيهَآ أَحْقَابًۭا",
        "verse_24": "لَّا يَذُوقُونَ فِيهَا بَرْدًۭا وَلَا شَرَابًا",
        "verse_25": "إِلَّا حَمِيمًۭا وَغَسَّاقًۭا",
        "verse_26": "جَزَآءًۭ وِفَاقًا",
        "verse_27": "إِنَّهُمْ كَانُوا۟ لَا يَرْجُونَ حِسَابًۭا",
        "verse_28": "وَكَذَّبُوا۟ بِـَٔايَٰتِنَا كِذَّابًۭا",
        "verse_29": "وَكُلَّ شَىْءٍ أَحْصَيْنَٰهُ كِتَٰبًۭا",
        "verse_30": "فَذُوقُوا۟ فَلَن نَّزِيدَكُمْ إِلَّا عَذَابًا",
        "verse_31": "إِنَّ لِلْمُتَّقِينَ مَفَازًا",
        "verse_32": "حَدَآئِقَ وَأَعْنَٰبًۭا",
        "verse_33": "وَكَوَاعِبَ أَتْرَابًۭا",
        "verse_34": "وَكَأْسًۭا دِهَاقًۭا",
        "verse_35": "لَّا يَسْمَعُونَ فِيهَا لَغْوًۭا وَلَا كِذَّٰبًۭا",
        "verse_36": "جَزَآءًۭ مِّن رَّبِّكَ عَطَآءً حِسَابًۭا",
        "verse_37": "رَّبِّ ٱلسَّمَٰوَٰتِ وَٱلْأَرْضِ وَمَا بَيْنَهُمَا ٱلرَّحْمَٰنِ ۖ لَا يَمْلِكُونَ مِنْهُ خِطَابًۭا",
        "verse_38": "يَوْمَ يَقُومُ ٱلرُّوحُ وَٱلْمَلَٰٓئِكَةُ صَفًّۭا ۖ لَّا يَتَكَلَّمُونَ إِلَّا مَنْ أَذِنَ لَهُ ٱلرَّحْمَٰنُ وَقَالَ صَوَابًۭا",
        "verse_39": "ذَٰلِكَ ٱلْيَوْمُ ٱلْحَقُّ ۖ فَمَن شَآءَ ٱتَّخَذَ إِلَىٰ رَبِّهِۦ مَـَٔابًا",
        "verse_40": "إِنَّآ أَنذَرْنَٰكُمْ عَذَابًۭا قَرِيبًۭا يَوْمَ يَنظُرُ ٱلْمَرْءُ مَا قَدَّمَتْ يَدَاهُ وَيَقُولُ ٱلْكَافِرُ يَٰلَيْتَنِى كُنتُ تُرَٰبًۢا"
    },
